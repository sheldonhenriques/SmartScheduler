{
    "manifest_version": 3,
    "name": "Network Logger Extension",
    "version": "1.0",
    "permissions": [
        "webRequest"
    ],
    "host_permissions": [
        "https://outlook.office365.com/owa/*"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html"
    }
}